{
  "name": "jscrewit",
  "version": "2.32.0",
  "description": "Converts plain JavaScript into JSFuck code, which consists of only six different characters: ! ( ) + [ ]",
  "keywords": [
    "esoteric",
    "jsfuck",
    "obfuscation"
  ],
  "homepage": "https://jscrew.it",
  "bugs": "https://github.com/fasttime/JScrewIt/issues",
  "license": "ISC",
  "author": "Francesco Trotta <ft@fasttime.org> (https://github.com/fasttime)",
  "files": [
    "Features.md",
    "JScrewIt.html",
    "api-doc",
    "lib",
    "screw.js",
    "tools",
    "ui",
    "!.*"
  ],
  "main": "lib/jscrewit.js",
  "types": "lib/jscrewit.d.ts",
  "bin": {
    "jscrewit": "screw.js"
  },
  "directories": {
    "lib": "lib",
    "doc": "doc",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fasttime/JScrewIt.git"
  },
  "scripts": {
    "build": "npm install && gulp",
    "docs": "gulp docs",
    "feature-info": "node dev/legacy/feature-info.js",
    "prepare": "node dev/patch-rollup.mjs",
    "test": "mocha --check-leaks --ui=ebdd test/{*,*/*}.spec.js"
  },
  "devDependencies": {
    "@babel/eslint-parser": "latest",
    "@babel/plugin-syntax-top-level-await": "latest",
    "@fasttime/lint": "latest",
    "@rollup/plugin-node-resolve": "latest",
    "acorn": "latest",
    "art-js": "latest",
    "basic-ftp": "latest",
    "c8": "latest",
    "chalk": "latest",
    "del": "latest",
    "ebdd": "latest",
    "eslint-plugin-ebdd": "latest",
    "expectations": "latest",
    "fast-glob": "latest",
    "gulp": "latest",
    "gulp-jsdoc3": "latest",
    "gulp-rename": "latest",
    "gulp-tap": "latest",
    "gulp-uglify": "latest",
    "handlebars": "latest",
    "inquirer": "latest",
    "mocha": "3",
    "mocha-bar": "latest",
    "postrequire": "latest",
    "rollup": "latest",
    "rollup-plugin-cleanup": "latest",
    "rollup-plugin-node-builtins": "latest",
    "rollup-plugin-node-globals": "latest",
    "tslib": "latest",
    "typedoc": "0.21",
    "typedoc-plugin-markdown": "latest",
    "typescript": "latest"
  },
  "workspaces": [
    "./packages/*"
  ]
}
